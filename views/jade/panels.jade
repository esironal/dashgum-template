                    .chart.mt
                      .sparkline(data-type='line', data-resize='true', data-height='75', data-width='90%', data-line-width='1', data-line-color='#fff', data-spot-color='#fff', data-fill-color='', data-highlight-line-color='#fff', data-spot-radius='4', data-data='[200,135,667,333,526,996,564,123,890,464,655]')
                    p.mt
                      b $ 17,980
                      br
                      | Month Income
                // /col-md-4
                .col-md-4.col-sm-4.mb
                  .stock.card
                    .stock-chart
                      #chart
                    .stock.current-price
                      .row
                        .info.col-sm-6.col-xs-6
                          abbr AAPL
                          time Yesterday
                        .changes.col-sm-6.col-xs-6
                          .value.up
                            i.fa.fa-caret-up.hidden-sm.hidden-xs
                            |  694.00
                          .change.hidden-sm.hidden-xs +4.95 (3.49%)
                    .summary
                      strong 18.3 M
                      span SHARES TRADED
                .col-md-4.col-sm-4.mb
                  .darkblue-panel.pn
                    .darkblue-header
                      h5 SITE STATICS
                    h1.mt
                      i.fa.fa-user.fa-3x
                    p + 1,789 NEW VISITS
                    footer
                      .centered
                        h5
                          i.fa.fa-trophy
                          |  17,988
                // /col-md-4
              // /END 5TH ROW OF PANELS
              .row
